{
    "name": "clash",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "node ./dist/index.js",
        "server": "nodemon ./dist/index.js",
        "watch": "tsc -w",
        "dev": "concurrently \"npm run watch\" \"npm run server\" \"npm run watch-copy\"",
        "build": "tsc && npm run copyFiles",
        "copyFiles": "copyfiles -u 1 \"./src/views/**/*\" dist/",
        "watch-copy": "nodemon --watch src/views -e ejs -x \"npm run copyFiles\"",
        "format": "prettier --write ."
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^6.1.0",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.34.5",
        "concurrently": "^9.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "ejs": "^3.1.10",
        "express": "^4.21.2",
        "express-fileupload": "^1.5.1",
        "express-rate-limit": "^7.5.0",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "nodemailer": "^6.9.16",
        "prisma": "^6.1.0",
        "socket.io": "^4.8.3",
        "uuid4": "^2.0.3",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.19",
        "@types/ejs": "^3.1.5",
        "@types/express": "^5.0.6",
        "@types/express-fileupload": "^1.5.1",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^22.19.11",
        "@types/nodemailer": "^6.4.17",
        "@types/uuid": "^10.0.0",
        "@types/uuid4": "^2.0.3",
        "@types/ws": "^8.18.1",
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.55.0",
        "copyfiles": "^2.4.1",
        "eslint": "^10.0.0",
        "nodemon": "^3.1.11",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.55.0"
    }
}
